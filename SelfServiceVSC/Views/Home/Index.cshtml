@using AAC.SelfServiceVSC
@using AAC.SelfServiceVSC.Models
@{
    ViewData["Title"] = "Home Page";
}
<style>
    /* Make sure the parent container does not trigger the click */
    .star-rating-wrapper {
        pointer-events: none; /* Disable interaction with the parent */
    }

    /* Make the stars container clickable */
    #dynamic-stars {
        display: flex;
        justify-content: center;
        align-items: center;
        width: 100%;
        height: 100px;
        cursor: pointer; /* Show the pointer cursor when hovering over the stars */
        pointer-events: auto; /* Enable interaction only with the stars */
    }


    /* Bigger Popup */
    #reviews-popup {
        display: none; /* Initially hidden */
        position: fixed; /* Position it at the center of the screen */
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%); /* Center the popup */
        background-color: white;
        border: 1px solid #ccc;
        box-shadow: 0px 4px 8px rgba(0, 0, 0, 0.2);
        padding: 30px; /* Increased padding for more space */
        z-index: 10;
        max-width: 80%; /* Increased width for more space */
        width: 90%; /* Wider for better visibility of reviews */
        height: 55%; /* Increased height to show more content */
        white-space: normal;
        border-radius: 10px;
    }

    /* Reviews displayed in a row */
    #reviews-list {
        display: flex;
        gap: 20px;
        flex-wrap: nowrap; /* Keep reviews in a row */
        overflow-x: auto; /* Horizontal scrolling if needed */
    }

    /* Review Item */
    .review-item {
        display: flex;
        flex-direction: column;
        align-items: center;
        text-align: center;
        width: 200px; /* Increased width for better layout */
        padding: 15px; /* Padding for spacing */
        border: 1px solid #ddd;
        border-radius: 10px;
        background-color: #f9f9f9;
        transition: transform 0.3s ease; /* Smooth transition for hover effect */
    }

        /* Hover effect for review item */
        .review-item:hover {
            transform: translateY(-10px); /* Slightly move the item up on hover */
        }

    /* Bigger Review Text */
    .review-text {
        margin: 5px 0;
        line-height: 1.5; /* Increased line height for better readability */
        font-size: 1em; /* Adjust font size for better visibility */
        color: #333; /* Darker text for readability */
        max-height: 100px; /* Limit the review text height */
        overflow-y: auto; /* Scroll vertically within the review if text is long */
    }

    /* Review Details */
    .review-details {
        overflow: hidden;
        max-height: 150px; /* Increased max-height to show more text */
        text-overflow: ellipsis;
        white-space: normal; /* Allow multi-line text */
        display: block;
        font-size: 0.95em; /* Slightly larger font size for better readability */
    }

    /* Image Styling */
    .review-photo {
        width: 80px; /* Increased size of images */
        height: 80px;
        border-radius: 50%;
        margin-bottom: 10px;
        object-fit: cover;
        border: 2px solid #ddd;
    }

        /* Error handling for images */
        .review-photo.error {
            background-color: #f0f0f0;
            color: #bbb;
            display: flex;
            justify-content: center;
            align-items: center;
            text-align: center;
            font-size: 14px;
            font-weight: bold;
            line-height: 1.2;
        }

    .review-stars {
        color: gold; /* Or use #FFD700 for a hex code equivalent */
        font-size: 20px; /* Adjust the size if needed */
        margin: 5px 0; /* Add spacing */
        text-align: center; /* Center the stars if needed */
    }

    /* Bigger stars and gold color */
    .star-rating {
        font-size: 36px; /* Increase size of the stars */
        color: gold; /* Gold color for stars */
    }

    /* Semi-transparent background for focus effect */
    body.popup-active {
        overflow: hidden; /* Prevent scrolling when popup is visible */
    }

        body.popup-active::before {
            content: '';
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: rgba(0, 0, 0, 0.5); /* Dim background effect */
            z-index: 9;
        }


    .spinner-container {
        position: fixed;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        display: flex;
        justify-content: center;
        align-items: center;
        background-color: rgba(0, 0, 0, 0.5); /* Semi-transparent black background */
    }

    #spinner {
        text-align: center;
        color: white;
    }

    .error-message {
        position: fixed;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
        background-color: white;
        border: 1px solid yellow;
        padding: 20px;
        z-index: 1000; /* Ensure it appears above other elements */
        display: none; /* Initially hide the error message */
    }

    .popup {
        position: fixed;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
        background-color: white;
        border: 1px solid orange;
        padding: 20px;
        z-index: 1000;
    }

    #popup .button-row {
        margin-top: 15px; /* Adjust the margin as needed */
    }

    #popup button {
        width: 50px !important; /* Adjust the width as needed */
        height: 25px !important; /* Adjust the height as needed */
        font-size: 14px !important; /* Adjust the font size as needed */
        background-color: orange; /* Set background color to orange */
        color: white; /* Set text color to white */
        border: none; /* Remove button border */
        border-radius: 5px; /* Apply border radius */
        cursor: pointer; /* Add cursor pointer */
    }

    #restart_popup {
        position: fixed;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
        background-color: white;
        border: 1px solid orange;
        padding: 20px;
        z-index: 1000;
        display: none; /* Hide initially */
    }

        #restart_popup .button-row {
            display: flex;
            justify-content: center; /* Centers the button horizontally */
            align-items: center; /* Centers the button vertically (if needed) */
            margin-top: 10px; /* Adjust spacing as needed */
        }

        #restart_popup button {
            width: 70px !important; /* Adjust the width as needed */
            height: 25px !important; /* Adjust the height as needed */
            font-size: 14px !important; /* Adjust the font size as needed */
            background-color: orange; /* Set background color to orange */
            color: white; /* Set text color to white */
            border: none; /* Remove button border */
            border-radius: 5px; /* Apply border radius */
            cursor: pointer; /* Add cursor pointer */
        }

</style>
<div class="container">
    <form id="estimate_form" method="post" action="/estimate">
        <div class="row">
            <div class="col-md-6">
                <h3 class="text-center">Coverage You Can Count On! <i class="fas fa-shield-alt color-dtcblue"></i></h3>
                <p class="text-center mt-4 mb-4" style="font-weight: bold;font-size: 1.2em;">Extend your factory warranty coverage out to 75,000, 100,000 or even 125,000 miles at a wholesale cost if you do it before your factory warranty runs out</p>
                <hr />
                <p class="text-center mt-4 mb-4" style="font-weight: bold;">Get exceptional vehicle protection at an incredible month-to-month cost:</p>
                <ul class="checkboxes">
                    <li style="font-weight: bold;"><i class="fas fa-check-circle"></i>Plans starting as low as $29.99/month</li>
                    <li style="font-weight: bold;">
                        <i class="fas fa-check-circle"></i>Pay in full today: <br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; If you pay in full receive a 10% discount on the whole contract
                    </li>
                    <li style="font-weight: bold;"><i class="fas fa-check-circle"></i>Get your free, no obligation quote in 15 seconds</li>
                    <li style="font-weight: bold;">
                        <i class="fas fa-check-circle"></i>Coverage Details
                        <ul>
                            <li><a href="#" class="whatiscovered"><i class="fas fa-question-circle"></i>What is Covered?</a></li>
                            <li>
                                <a href="#" class="whatiscoverednot"><i class="fas fa-question-circle"></i>What is Not Covered?</a>
                            </li>
                            <li><a href="#" class="whatiscoveredchart"><i class="fas fa-question-circle"></i>Coverage Chart</a></li>
                        </ul>
                    </li>
                    <li style="font-weight: bold;">
                        <i class="fas fa-check-circle"></i>Choose the term that best fits your needs:
                        <ul>
                            <li>
                                <i class="fas fa-check-circle"></i><span class="fw-bold" style="font-size: 1.2em;">Flexible pricing</span><br />Your monthly
                                cost is based on the year, make, model and mileage of your vehicle. Prices start as low as $30 per
                                month.
                            </li>
                            <li>
                                <i class="fas fa-check-circle"></i><span class="fw-bold" style="font-size: 1.2em;">Easy, no-obligation quote</span><br />Enter
                                your Vehicle Identification Number (VIN) and current mileage to get your quote. Coverage starts as soon
                                as you sign up.
                            </li>
                            <li>
                                <i class="fas fa-check-circle"></i><span class="fw-bold" style="font-size: 1.2em;">Reliable protection</span><br />Eliminate
                                large, unexpected repair bills. Peace-of-mind comes from knowing that your budget won’t get hit by huge
                                surprises.
                            </li>
                        </ul>
                    </li>
                </ul>
                <div class="star-rating-wrapper">
                    <div id="dynamic-stars" class="star-rating" style="margin: 0; padding: 0;"></div>
                    <p style="margin: 0; padding: 0; text-align: center; font-size: 18px; font-weight: bold;">
                        <span id="ratings" style="font-size: 20px; font-weight: bold;"></span>
                        Google review Rating from
                        <span id="user-ratings-total" style="font-size: 20px; font-weight: bold;"></span>
                        total Reviews
                    </p>

                </div>


                <!-- Reviews Popup -->
                <div id="reviews-popup">
                    <ul id="reviews-list"></ul>
                </div>
            </div>
            <div class="col-md-6">
                <div class="card transparent">
                    <div class="card-header">
                        <h3 class="text-center">Get Your Free Quote</h3>
                    </div>
                    <div class="card-body">
                        <p class="form-section">Fill in some information about your vehicle to see available coverage plans!</p>
                        <div class="form-group">
                            <label for="form_vin">VIN</label>
                            <input type="text" name="vin" id="form_vin" class="form-control" value=""
                                   onblur="callVinDecoder()" onkeypress="checkEnter(event)" placeholder="Type your vehicle's full VIN" />
                        </div>
                        <div class="form-group">
                            <label for="form_year">Year</label>
                            @* <select name="year" id="form_year" class="form-control" readonly>
								<option selected value="">Select</option>
								@{
									for (Int32 i = -1; i < 5; i++)
									{
										<text>
											<option value="@(DateTime.Now.Year - i)">@(DateTime.Now.Year - i)</option>
										</text>
									}
								}
							</select> *@
                            <input type="text" name="year" id="form_year" class="form-control" value=""
                                   placeholder="Select" readonly />
                        </div>
                        <div class="form-group">
                            <label for="form_make">Make</label>
                            <input name="make" id="form_make" class="form-control" placeholder="Select" readonly>
                            @*<option selected value="">Select</option>*@
                            </input>
                        </div>
                        <div class="form-group">
                            <label for="form_model">Model</label>
                            <input name="model" id="form_model" class="form-control" placeholder="Select" readonly>
                            @* <option selected value="">Select</option> *@
                            @* <option selected value="">Choose a Make</option> *@
                            </input>
                        </div>
                        <hr />
                        <div class="form-group">
                            <label for="form_mileage">Current Odometer Reading</label>
                            <input type="text" name="mileage" id="form_mileage" class="form-control" value=""
                                   placeholder="Type your current odometer reading" data-inputmask="'regex': '\\d{1,7}'" inputmode="numeric" />
                        </div>
                        <div class="form-group" style="margin-top:20px">
                            <div class="custom-control custom-checkbox d-flex align-items-center">
                                <input type="checkbox" id="agree_terms" name="agree_terms" class="custom-control-input">
                                <label class="custom-control-label" for="agree_terms" style="margin-left:5px; font-size: 14px; font-weight: 500; color: #333;">
                                    I agree to the
                                    <a href="/home/termsandconditions" target="_blank" style="color: #333; font-weight: bold; text-decoration: underline; cursor:pointer">Terms and Conditions</a>.
                                </label>
                            </div>
                            <span id="agree_terms-error" class="error text-danger mt-1" style="display: none; font-size: 12px;"></span>
                        </div>

                        <br />
                        <hr />
                        <div class="text-center">
                            <button id="submit_estimate" type="button" title="Get a FREE Quote" class="btn btn-success rounded-pill p-4 pt-2 pb-2">
                                Get a FREE
                                Quote
                            </button>
                        </div>
                        <br />
                        <hr />
                        <p class="text-center" style="margin: 0;">Need Help?</p>
                        <p class="text-center" style="margin: 0;">
                            <a href="tel:@SelfServiceVSC.AppSettings.PhoneNumberMain">
                                <i class="fas fa-phone-alt small m-1"></i>@SelfServiceVSC.AppSettings.PhoneNumberMain.FormatPhone()
                            </a>
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </form>
</div>
<div id="popup" class="popup" style="display: none;">
    <p>A quote has already been saved for this vehicle – <br /> would you like to continue where you left off or start a new quote?</p>
    <div class="button-row row">
        <div class="col-12 text-center">
            <button id="yesButton">Yes</button>
            <button id="noButton">No</button>
        </div>
    </div>
</div>

<div id="restart_popup" class="restartPopup" style="display: none;">
    <h4>No Coverage Plans Available<br /></h4>
    <p>We're sorry, but your vehicle is not eligible for coverage at this time. If you have another vehicle you would like to cover, please click “Restart” below.</p>
    <div class="button-row row text-center">
        <button id="restartButton" onclick="resetForm()">Restart</button>
    </div>
</div>

